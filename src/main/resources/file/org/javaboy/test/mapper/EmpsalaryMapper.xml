<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.javaboy.test.mapper.EmpsalaryMapper">

    <resultMap id="BaseResultMap" type="org.javaboy.test.model.Empsalary">
            <result column="id" property="id" jdbcType="INTEGER" />
            <result column="eid" property="eid" jdbcType="INTEGER" />
            <result column="sid" property="sid" jdbcType="INTEGER" />
    </resultMap>

    <select id="getAllEmpsalarys" resultMap="BaseResultMap">
        select * from empsalary t1 left join t2  on t1.id= t2.;
    </select>
</mapper>